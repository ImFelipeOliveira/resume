{
  "name": "resume",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "Um bot de WhatsApp que resume conversas de grupo.",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && concurrently \"npm:start:bot\" \"npm:start:worker\"",
    "start:prod": "concurrently \"npm:start:bot\" \"npm:start:worker\"",
    "start:bot": "node dist/bot.js",
    "start:worker": "node dist/worker.js",
    "dev": "concurrently --kill-others-on-fail \"npm:dev:bot\" \"npm:dev:worker\"",
    "dev:bot": "ts-node-dev --respawn --transpile-only src/bot.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@whiskeysockets/baileys": "^6.7.18",
    "amqplib": "^0.10.9",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-cron": "^4.2.1",
    "openai": "^5.15.0",
    "pino": "^9.9.0",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/estree": "^1.0.8",
    "@types/express": "^5.0.3",
    "@types/json-schema": "^7.0.15",
    "@types/long": "^4.0.2",
    "@types/node": "^24.3.0",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "@types/strip-bom": "^3.0.0",
    "@types/strip-json-comments": "^0.0.30",
    "ts-node-dev": "^2.0.0"
  }
}
